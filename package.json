{
  "name": "@mantoso/github-semantic-release-config",
  "description": "semantic-release shareable config to create GitHub releases.",
  "version": "1.1.8",
  "author": "@reavon (https://github.com/reavon)",
  "bugs": {
    "url": "https://github.com/mantoso/github-semantic-release-config/issues"
  },
  "contributors": [
    "@reavon (https://github.com/reavon)"
  ],
  "dependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1"
  },
  "devDependencies": {
    "semantic-release": "^22.0.1",
    "xo": "^0.59.0"
  },
  "engines": {
    "node": ">=10.18"
  },
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "homepage": "https://github.com/terraform/github-semantic-release-config#readme",
  "keywords": [
    "conventional-changelog",
    "release",
    "semantic-release",
    "semantic-release-config",
    "version"
  ],
  "license": "MIT",
  "peerDependencies": {
    "semantic-release": ">=16.0.0"
  },
  "prettier": {
    "printWidth": 120,
    "trailingComma": "es5"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mantoso/github-semantic-release-config.git"
  },
  "scripts": {
    "semantic-release": "semantic-release",
    "test": "xo"
  },
  "xo": {
    "prettier": true,
    "space": true,
    "rules": {
      "unicorn/string-content": "off"
    }
  }
}
